<html metal:use-macro="here/content_lib_master/macros/master">
  <metal:block fill-slot="main"
               tal:define="tags here/getRegisteredTags">
    <h1 i18n:translate="cpswiki_specialtags">Special tags</h1>
    <div tal:repeat="tag tags">
      <tal:block tal:define="tag_id python:tag[0];tagger python:tag[1]">
        <strong tal:content="tag_id"/>
        <p i18n:translate="" tal:content="structure tagger"/>
      </tal:block>
    </div>
  </metal:block>
</html>